{
  "name": "goferfs",
  "description": "Filesystem Abstraction Library",
  "version": "1.0.0-8",
  "main": "dist/index.js",
  "types": "dist/index.ts",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:lint": "tslint src/**/*.ts",
    "lint:types": "tsc --noEmit --skipLibCheck -p tsconfig.json",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel src --out-dir=dist --extensions='.ts' --source-maps",
    "test": "jest",
    "prepublish": "npm test"
  },
  "repository": {
    "url": "http://github.com/matchbooklab/goferfs",
    "type": "git"
  },
  "author": "Cameron Spear <cameron@matchbooklab.com>",
  "contributors": [
    "Justen Martin <jmart@the-coder.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matchbooklab/goferfs/issues"
  },
  "homepage": "https://github.com/matchbooklab/goferfs#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/mime": "^2.0.1",
    "@types/node": "^12.12.14",
    "jest": "^24.9.0",
    "np": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
